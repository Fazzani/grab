    <credits>
      <actor role="Fernanda del Castillo / Victoria Villafañe">Aylin Mujica</actor>
    </credits>
    <category lang="fr">Feuilleton sentimental</category>
  </programme>
  <programme start="20210114025000 +0100" stop="20210114034000 +0100" channel="Novelas TV">
